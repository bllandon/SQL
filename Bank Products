#1 Provide list of all products available at the bank
SELECT product_name
FROM bank_products;

#2 Provide list of bank products that are either Checking or Savings accounts
SELECT *
FROM bank_products
WHERE product_type = 'Savings'
OR product_type = 'Checking';

#3 Provide name and interest rate of bank products that are savings accounts, have an interest rate of at least 0.75, and no monthly fee
SELECT product_name, interest_rate
FROM bank_products
WHERE product_type='Savings'
AND interest_rate >= 0.75
AND monthly_fee = 0.00;
